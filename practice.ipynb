    "# question no 396\n",
    "\n",
    "nums = [4,3,2,6]\n",
    "n = []\n",
    "def rev(arr, n):\n",
    "        arr.insert(0, arr[len(arr)-1])\n",
    "        arr.pop()\n",
    "        n = arr\n",
    "        return n\n",
    "        \n",
    "\n",
    "for i in range(len(nums)):\n",
    "        print(rev(nums, n))"
   ]
